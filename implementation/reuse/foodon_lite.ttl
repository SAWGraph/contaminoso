                    rdfs:subClassOf obo:FOODON_03412113 ;
                                    #obo:FOODON_00001291 , 
                    obo:IAO_0000114 obo:IAO_0000122 ;
                    obo:IAO_0000115 "Clam is a common name for several kinds of bivalve molluscs. The word is often applied only to those that are edible and live as infauna, spending most of their lives halfway buried in the sand of the seafloor."@en ;
                    obo:IAO_0000119 "https://en.wikipedia.org/wiki/Clam"^^xsd:anyURI ;
                    dcterms:contributor "Damion Dooley"@en ;
                    schema:image "https://upload.wikimedia.org/wikipedia/commons/3/3b/LittleNeck_clams_USDA96c1862.jpg"@en ;
                    go:hasDbXref "langual:B1331" ;
                    rdfs:label "clam"@en .

###  http://purl.obolibrary.org/obo/FOODON_03411223
